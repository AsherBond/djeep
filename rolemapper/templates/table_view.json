{
"{{ info.obj }}": [
    {% for object in info.objects %}
    {
      {% for field_name in info.field_names %}
        {% if field_name != 'hardware_info' %}
          "{{ field_name }}": "{{ object[field_name] }}",
        {% else %}
          "{{ field_name }}": {{ object[field_name] }},
        {% endif %}
      {% endfor %}
    },
    {% endfor %}
  ]
}
